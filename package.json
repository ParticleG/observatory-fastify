    "@fastify/websocket": "^8.2.0",
    "ansi-colors": "^4.1.3",
    "chalk": "^5.3.0",
    "fastify": "^4.21.0",
    "fastify-cli": "^5.8.0",
    "fastify-graceful-shutdown": "^3.5.1",
    "fastify-plugin": "^4.5.1",
    "toml": "^3.0.0",
    "ts-date": "^3.1.0",
    "uiohook-napi": "^1.5.1",
    "uuid": "^9.0.0"
